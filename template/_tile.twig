{#
    Превращает тайлы, переданные массивами типа {number : '5', color : 'z'} в картинки.
    Параметры:
        tiles
#}
{% spaceless %}
    {% for tile in tiles %}
        <img
            src="http://tenhou.net/2/t/{{ tile.toTenhou }}.gif"
            alt="{{ tile.toRussian }}"
            title="{{ tile.toRussian }}"
            class="tile"
        />
    {% endfor %}
{% endspaceless %}
